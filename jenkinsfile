pipeline {
    agent any
    triggers {
        upstream(
            upstreamProjects: "Integration Test  Smoke",
            threshold: hudson.model.Result.ABORTED
        )
    }    
    stages {
        stage('Start') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
