pipeline {
    agent any
    triggers {
        upstream "IntegrationTestSmoke"
        // upstream "my-upstream-project1,my-upstream-project2"
    }  
    stages {
        stage('Start') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
