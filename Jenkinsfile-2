pipeline {
    agent any
    parameters{
        string(name: 'FROM_BUILD', defaultValue: '', description: 'Build Source')
        booleanParam(name: 'IS_READY', defaultValue: false, description: 'Is Ready for prod?')
    }
    stages {
        stage("Deploy") {
            steps {
                echo "Build deploy from Source ${params.FROM_BUILD}"
            }
        }

    }
}
